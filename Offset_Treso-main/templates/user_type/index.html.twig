{% extends 'base.html.twig' %}

{% block title %}UserType index{% endblock %}

{% block body %}
    <h1>UserType index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Code</th>
                <th>Description</th>
                <th>Created_at</th>
                <th>Updated_at</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for user_type in user_types %}
            <tr>
                <td>{{ user_type.id }}</td>
                <td>{{ user_type.code }}</td>
                <td>{{ user_type.description }}</td>
                <td>{{ user_type.createdAt ? user_type.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ user_type.updatedAt ? user_type.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_user_type_show', {'id': user_type.id}) }}">show</a>
                    <a href="{{ path('app_user_type_edit', {'id': user_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_user_type_new') }}">Create new</a>
{% endblock %}
