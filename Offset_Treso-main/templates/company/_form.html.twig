{# FORMULAIRE D'ENREGISTREMENT D'UNE SOCIETE #}
<div class="row">
    <div class="col-12">
        <div class="form-group row mb-3 mt-3">
            <label class="col-md-2 col-form-label required">
                Date début:
            </label>
            <div class="col-md-2">
                {{ form_widget(form.Annee_debut_activ) }}
            </div>

            <label class="col-md-2 col-form-label required" title="Prénom du gérant où de la gérante">
                Gérant:
            </label>
            <div class="col-md-2">
                {{ form_widget(form.Gerant) }}
            </div>
            <label class="col-md-2 col-form-label required" title="Prénom du gérant où de la gérante">
                Capital social :
            </label>
            <div class="col-md-2">
                {{ form_widget(form.Capital_social) }}
            </div>
        </div>
    </div>
</div>
