{% extends 'base.html.twig' %}

{% block title %}CashAccount{% endblock %}

{% block body %}
    <h1>CashAccount</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cash_account.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ cash_account.nom }}</td>
            </tr>
            <tr>
                <th>Num_cpte</th>
                <td>{{ cash_account.numCpte }}</td>
            </tr>
            <tr>
                <th>Solde_ouv</th>
                <td>{{ cash_account.soldeOuv }}</td>
            </tr>
            <tr>
                <th>Plafond</th>
                <td>{{ cash_account.plafond }}</td>
            </tr>
            <tr>
                <th>Solde_plancher</th>
                <td>{{ cash_account.soldePlancher }}</td>
            </tr>
            <tr>
                <th>Caissier</th>
                <td>{{ cash_account.caissier }}</td>
            </tr>
            <tr>
                <th>Contact</th>
                <td>{{ cash_account.contact }}</td>
            </tr>
            <tr>
                <th>Responsable</th>
                <td>{{ cash_account.responsable }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ cash_account.createdAt ? cash_account.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ cash_account.updatedAt ? cash_account.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_cash_account_index') }}">back to list</a>

    <a href="{{ path('app_cash_account_edit', {'id': cash_account.id}) }}">edit</a>

    {{ include('cash_account/_delete_form.html.twig') }}
{% endblock %}
